{
  "buildCommand": "cd frontend && flutter build web --release",
  "outputDirectory": "frontend/build/web",
  "framework": null,
  "installCommand": "",
  "rewrites": [
    {
      "source": "/((?!api/.*).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
